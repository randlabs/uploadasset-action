{
  "name": "uploadasset-action",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub action to upload assets to existing releases",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "package": "node ./node_modules/@vercel/ncc/dist/ncc/cli.js build --source-map --license licenses.txt",
    "all": "npm run lint && npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/randlabs/uploadasset-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "RandLabs Inc.",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.3",
    "glob": "^8.0.3",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.6.3",
    "@typescript-eslint/parser": "^5.37.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-jest": "^26.7.0",
    "js-yaml": "^4.1.0",
    "typescript": "^4.7.4"
  }
}
