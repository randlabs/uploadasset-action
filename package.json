{
  "name": "uploadasset-action",
  "version": "2.0.0",
  "private": true,
  "description": "GitHub action to upload assets to existing releases",
  "main": "lib/main.js",
  "scripts": {
    "prebuild": "rimraf -I lib dist",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "package": "node ./node_modules/@vercel/ncc/dist/ncc/cli.js build --minify --no-source-map-register --license licenses.txt",
    "all": "npm run lint && npm run build && npm run package",
    "test": "jest --testTimeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/randlabs/uploadasset-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "RandLabs Inc.",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "glob": "^10.3.10",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "octokit": "^3.1.1",
    "proxy-agent": "^6.3.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.5",
    "@types/mime-types": "^2.1.2",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.50.0",
    "eslint-plugin-jest": "^27.4.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
